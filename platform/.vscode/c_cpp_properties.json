{
  "configurations": [
    {
      "name": "Mac",
      "includePath": ["${workspaceFolder}/**", 
                      "${workspaceFolder}/", 
                      "/opt/homebrew/include", 
                      "/usr/local/**", 
                      "/opt/homebrew/opt/python@3.11/Frameworks/Python.framework/Versions/3.11/include/python3.11"],
      "defines": [],
      "macFrameworkPath": ["/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "clang-x64"
    },
    {
      "name": "Linux",
      "includePath": ["${workspaceFolder}/**", "${workspaceFolder}/", "/usr/local/**"],
      "defines": [],
      "compilerPath": "/usr/bin/clang++",
      "cStandard": "c11",
      "cppStandard": "c++20",
      "intelliSenseMode": "linux-clang-x64",
      "configurationProvider": "ms-vscode.makefile-tools"
    }
  ],
  "version": 4
}
